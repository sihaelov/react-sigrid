{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true
	},
	"parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
	"plugins": [
		"react"
	],
	"extends": ["eslint:recommended", "plugin:react/recommended"]
}